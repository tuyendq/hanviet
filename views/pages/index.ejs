<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <% include ../partials/head %>    
  </head>
  <body>
    <header>
      <% include ../partials/header %>    
    </header>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <h2>Characters</h2>
    <ul>  
      <% characters.forEach(function(character) { %>
        <li><%= character.id %> - <%= character.ch %></li>
    <% }); %>      
    </ul>

    <footer>
      <% include ../partials/footer %>    
    </footer>
  </body>
</html>
